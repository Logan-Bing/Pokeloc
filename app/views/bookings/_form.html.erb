
  <%= simple_form_for [pokemon, booking] do |f| %>
      <%= f.input :start_date, label: "Date de début", as: :string, input_html: { data: { controller: "datepicker" } } %>
      <%= f.input :end_date, label: "Date de fin", as: :string, input_html: { data: { controller: "datepicker" } } %>
      <%= f.submit "Réserver", class:"p-submit"%>
  <% end %>
