

<div class="container">
<h1>New booking</h1>
<%= simple_form_for [@pokemon, @booking] do |f| %>
<div>
  <%= f.input :start_date,
  as: :string,
  input_html: { data: { controller: "datepicker" } } %>
</div>

<div>
  <%= f.input :end_date,
  as: :string,
  input_html: { data: { controller: "datepicker" } } %>
</div>

  <%= f.submit %>

<% end %>
</div>
